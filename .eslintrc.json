{
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "prettier/prettier": "error"
    }
  }
  